{
  "components/header/header": "<header class=\"site-header logo-align-{{theme_settings.global-header-logo-alignment}}\" role=\"banner\">\n  <div data-alerts>\n    {{#each product.reviews.messages}}\n      {{#if error}}\n        {{> core/alerts/alert-error message=error dismissable=true}}\n      {{/if}}\n      {{#if success}}\n        {{> core/alerts/alert-success message=success dismissable=true}}\n      {{/if}}\n    {{/each}}\n    {{#if cart.status_messages}}\n      {{#each cart.status_messages}}\n        {{#if type '==' 0}}\n          {{> core/alerts/alert-error message=message dismissable=true}}\n        {{/if}}\n        {{#if type '==' 1}}\n          {{> core/alerts/alert-success message=message dismissable=true}}\n        {{/if}}\n        {{#if type '==' 3}}\n          {{> core/alerts/alert-error message=message dismissable=true}}\n        {{/if}}\n      {{/each}}\n    {{/if}}\n  </div>\n\n  <section class=\"top-bar\">\n    <div class=\"site-alerts\" {{#if theme_settings.global-header-logo-alignment '==' 'center'}} data-cart-promos{{/if}}>\n      {{> components/header/shipping-messages}}\n    </div>\n    <div class=\"container\">\n      <div class=\"top-bar-center\">\n        {{#if theme_settings.global-header-logo-alignment '!=' 'center'}}\n          <div data-cart-promos>\n            {{> components/header/shipping-messages}}\n          </div>\n        {{/if}}\n        <div style=\"max-width: 300px;\">\n        {{> components/header/site-branding}}\n      </div>\n      </div>\n      <button class=\"button-mobile-nav-toggle {{#if cart.status_messages}} has-promo {{/if}}\">\n        <i class=\"burger\"></i>\n      </button>\n    </div>\n  </section>\n\n  <section class=\"main-nav-bar\">\n    <div class=\"container\">\n      {{#if theme_settings.global-header-logo-alignment '===' 'left'}}\n        {{> components/header/site-navigation-left-align}}\n      {{/if}}\n      {{#if theme_settings.global-header-logo-alignment '===' 'center'}}\n        {{> components/header/site-navigation-center-align}}\n      {{/if}}\n      {{#if theme_settings.global-header-logo-alignment '===' 'right'}}\n        {{> components/header/site-navigation-right-align}}\n      {{/if}}\n\n      <div class=\"header-buttons\">\n        <div class=\"button-group\">\n          <div class=\"repeat-order-container\">\n            <a href=\"https://unicater.co.uk/contact-form/\">\n              <button class=\"repeat-order-button\">Contact Us</button>\n            </a>\n          </div>\n          <div class=\"deals-container\">\n            <a href=\"/deals\">\n              <button class=\"repeat-order-button\">Deals</button>\n            </a>\n          </div>\n        </div>\n        <div class=\"customer-links-container\">\n          {{#if settings.account_creation_enabled}}\n          <div class=\"customer-links\">\n            {{#if customer}}\n              {{#contains page_type 'account'}}\n                <a class=\"top-bar-button\" href=\"{{urls.auth.logout}}\">\n                  {{> svg/singout}}\n                </a>\n              {{else}}\n                <a class=\"top-bar-button\" href=\"{{urls.account.index}}\">\n                  {{> svg/account}}\n                </a>\n              {{/contains}}\n            {{else}}\n              <a class=\"top-bar-button\" href=\"{{urls.auth.login}}\">\n                {{> svg/account}}\n              </a>\n            {{/if}}\n          </div>\n        {{/if}}\n        {{#if settings.show_wishlist}}\n        <a class=\"top-bar-button button-wishlist\" href=\"{{urls.account.wishlists.all}}\">\n          {{> svg/wishlist}}\n        </a>\n        {{/if}}\n        <span class=\"button-mobile-nav-cart {{#if cart.status_messages}} has-promo {{/if}}\">\n          <a href=\"{{urls.cart}}\">\n            {{> components/header/cart-icon}}\n          </a>\n        </span>\n        <button class=\"top-bar-button button-cart-toggle {{#unless settings.show_wishlist}} no-wishlist {{/unless}}\">\n          {{> components/header/cart-icon}}\n        </button>\n        </div>\n      </div>\n    </div>\n  </section>\n</header>\n\n<style>\n\n.button-group {\n  display: flex;\n  align-items: center;\n}\n\n.repeat-order-container{\n  margin-right: 10px;\n}\n\n.deals-container {\n  margin-right: 10px;\n  width: 80px;\n}\n\n.repeat-order-button {\n  border: 0;\n  padding: 12.5px 5px;\n  border-radius: 8px;\n  background-color: #36AF74;\n  color: #ffffff;\n  font-weight: bold;\n  transition: all 0.5s;\n  -webkit-transition: all 0.5s;\n  cursor: pointer;\n  text-align: center; /* Center the text within the buttons */\n  height: 35px; /* Set a fixed height */\n  display: flex;\n  align-items: center; /* Ensure content is vertically centered */\n}\n\n.repeat-order-button:hover {\n  background-color: rgba(0, 0, 0, 0.1); /* Change to a semi-transparent background */\n  color: rgb(252,110,86);\n\n}\n\n.repeat-order-button:active {\n  background-color: #36AF74;\n  transition: all 0.25s;\n  -webkit-transition: all 0.25s;\n  box-shadow: none;\n  transform: scale(0.98);\n}\n\n.customer-links-container {\n  display: flex;\n  align-items: center;\n  gap: 2px;\n}\n</style>\n",
  "core/alerts/alert-error": "<div class=\"alert alert-error\">\n  {{#if dismissable}}<a class=\"alert-dismiss\">&times;</a>{{/if}}\n\n  <div class=\"alert-message\">\n    {{#if message}}\n      {{{message}}}\n    {{else}}\n      {{{this}}}\n    {{/if}}\n  </div>\n</div>\n",
  "core/alerts/alert-success": "<div class=\"alert alert-success\">\n  {{#if dismissable}}<a class=\"alert-dismiss\">&times;</a>{{/if}}\n\n  <div class=\"alert-message\">\n    {{#if message}}\n      {{{message}}}\n    {{else}}\n      {{{this}}}\n    {{/if}}\n  </div>\n</div>\n",
  "components/header/shipping-messages": "{{#if shipping_messages}}\n  {{#each shipping_messages}}\n    <div class=\"promo-message\">{{{message}}}</div>\n  {{/each}}\n{{/if}}\n\n{{#if cart.status_messages}}\n  {{#each cart.status_messages}}\n    {{#if type '==' 2}}\n      <div class=\"promo-message\">{{{message}}}</div>\n    {{/if}}\n  {{/each}}\n{{/if}}\n\n{{#if product.shipping_messages}}\n  {{#each product.shipping_messages}}\n    <div class=\"promo-message\">{{{message}}}</div>\n  {{/each}}\n{{/if}}\n",
  "components/header/site-branding": "<h1 class=\"site-branding {{#if settings.store_logo.image}}has-logo{{/if}}\">\n  <a class=\"site-branding-link\" href=\"{{urls.home}}\">\n    {{#if settings.store_logo.image }}\n      <img class=\"site-branding-logo\" src=\"{{getImage settings.store_logo.image 'logo'}}\" alt=\"{{settings.store_logo.title}}\">\n    {{else}}\n      <span class=\"site-branding-title\">{{settings.store_logo.title}}</span>\n    {{/if}}\n  </a>\n</h1>\n",
