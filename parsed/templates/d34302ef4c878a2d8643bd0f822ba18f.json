  "components/common/main-navigation-categories": "<div>\n  <div class=\"mega-nav-variant-container\">\n    <ul class=\"mega-nav-root-list\">\n      {{#each children}}\n        <li class=\"mega-nav-root-item\" data-category-id=\"{{id}}\" {{#if children}}data-mega-nav-root-item{{/if}}>\n          <a href=\"{{url}}\">{{name}}</a>\n        </li>\n      {{/each}}\n    </ul>\n    <div class=\"mega-nav-child-container\">\n      {{! For each root item, display a child container if it exists}}\n      {{#each children}}\n      {{#if children}}\n      <div class=\"mega-nav-child-list-container\" data-root-category-id=\"{{id}}\">\n        <div class=\"mega-nav-child-list-inner-container\">\n          <ul class=\"mega-nav-child-list\" data-root-category-id=\"{{id}}\">\n            <div class=\"category-row\">\n              {{#each children}}\n              <a href=\"{{url}}\" class=\"category-box-link\">\n                <div class=\"category-box\">\n                  <div class=\"outer-box\">\n                    <div class=\"image-link-container\">\n                      <img src='{{getImage image \"thumbnail\"}}' alt=\"{{name}}\">\n                      <span style=\"font-size:small; font-weight: 700;\" class=\"category-name\">{{name}}</span>\n                    </div>\n                  </div>\n                </div>\n              </a>\n              {{/each}}\n            </div>\n          </ul>\n          <a href=\"{{url}}\" data-root-category-id=\"{{id}}\" class=\"category-name\">Shop {{lang 'common.navigation_all'}} {{name}}</a>\n        </div>\n      </div>      \n        {{> components/common/navigation-mega-child-recursive}}\n      {{else}}\n        <div class=\"mega-nav-child-list-container\" data-root-category-id=\"{{id}}\">\n          <div class=\"mega-nav-child-list-inner-container\">\n            <ul class=\"mega-nav-child-list\" data-root-category-id=\"{{id}}\">\n              <div class=\"category-row\">\n                  <a href=\"{{url}}\" class=\"category-box-link\">\n                    <div class=\"category-box\">\n                      <div class=\"outer-box\">\n                        <div class=\"image-link-container\">\n                          <img src='{{getImage image \"thumbnail\"}}' alt=\"{{name}}\">\n                          <span style=\"font-size:small; font-weight: 700; \" class=\"category-name\">{{name}}</span>\n                        </div>\n                      </div>\n                    </div>\n                  </a>\n              </div>\n            </ul>\n            <a href=\"{{url}}\" data-root-category-id=\"{{id}}\" class=\"category-name\">Shop {{lang 'common.navigation_all'}} {{name}}</a>\n        </div>\n        </div>\n        {{> components/common/navigation-mega-child-recursive}}\n      {{/if}}\n      {{/each}}\n    </div>\n  </div>\n </div>\n  <style>\n.category-row {\n  display: flex;\n  flex-wrap: wrap;\n  gap: 10px;\n}\n\n.category-box {\n  flex: 1 1 50%; /* 50% width with margin between boxes */\n  box-sizing: border-box; /* Include padding and border in the box's total width and height */\n}\n\n.category-box-link {\n  display: block;\n  text-decoration: none;\n  font-size: 20px;\n}\n\n.outer-box {\n  border: 0.1px solid #ccc;\n  padding: 10px;\n  height: 100%;\n  border-radius: 10px;\n  text-align: center;\n  background-color: #f6f8fb; /* Add rounded corners */\n  height: 100px; /* Fixed height */\n  transition: all 0.3s ease; /* Smooth transition effect */\n}\n\n.outer-box:hover {\n  background-color: #0e8366 ; /* Change background color on hover */\n  cursor: pointer; /* Change cursor to pointer */\n}\n\n.image-link-container {\n  display: flex;\n  align-items: center;\n  margin-top: 5px;\n}\n\n.image-link-container img {\n  max-width: 100px;\n  max-height: 70px;\n  margin-right: 30px; /* Add margin between image and text */\n}\n\n.image-link-container a {\n  margin-left: auto; /* Push text to the right */\n  padding-right: 10px; /* Add padding to the right */\n  font-size: 16px; /* Add padding to the right */\n}\n\n.mega-nav-child-list-inner-container {\n  display: flex;\n  flex-direction: column;\n  height: 100%; /* Full height for the inner container */\n}\n\n.mega-nav-child-list {\n  flex: 1; /* Take available space */\n  overflow-y: auto; /* Enable vertical scrolling */\n}\n\n.category-name {\n  display: block;\n  text-align: center;\n  margin-top: 10px;\n  font-size: small;\n  font-weight: 700;\n}\n\n@media only screen and (min-width: 1710px) {\n  .outer-box {\n    width: 400px; /* Adjust width for larger screens */\n  }\n}\n\n@media only screen and (min-width: 1600px) and (max-width: 1710px) {\n  .outer-box {\n    width: 350px; /* Adjust width for screens between 1323px and 1700px */\n  }\n}\n\n@media only screen and (min-width: 1323px) and (max-width: 1600px) {\n  .outer-box {\n    width: 300px; /* Adjust width for screens between 1323px and 1700px */\n  }\n}\n\n@media only screen and (min-width: 1060px) and (max-width: 1322px) {\n  .outer-box {\n    width: 230px; /* Adjust width for screens between 1000px and 1322px */\n  }\n}\n\n@media only screen and (min-width: 1000px) and (max-width: 1060px) {\n  .outer-box {\n    width: 200px; /* Adjust width for screens between 1000px and 1322px */\n  }\n}\n\n  </style>\n  ",
  "components/common/navigation-mega-child-recursive": "{{#each children}}\n  {{#if children}}\n    <div class=\"mega-nav-child-list-container\" data-root-category-id=\"{{id}}\">\n      <span class=\"mega-nav-child-list-back\">\n         {{> svg/arrow-down}} <a href=\"#\" data-mega-nav-find-parent>Back</a>\n      </span>\n      <h2 class=\"mega-nav-child-header\">{{name}}</h2>\n      <div class=\"mega-nav-child-list-inner-container\">\n        <ul class=\"mega-nav-child-list\" >\n          <li class=\"mega-nav-child-item\"><a href=\"{{url}}\">All {{name}}</a></li>\n          {{#each children}}\n            {{#if children}}\n              <li class=\"mega-nav-child-item\" data-mega-nav-child-id=\"{{id}}\" data-mega-nav-find-child>\n                <a href=\"{{url}}\">{{name}}</a>\n                <span class=\"mega-nav-arrow\">{{> svg/arrow-down}}</span>\n              </li>\n            {{else}}\n              <li class=\"mega-nav-child-item\"><a href=\"{{url}}\">{{name}}</a></li>\n            {{/if}}\n          {{/each}}\n        </ul>\n      </div>\n    </div>\n    {{> components/common/navigation-mega-child-recursive}}\n  {{/if}}\n{{/each}}\n",
