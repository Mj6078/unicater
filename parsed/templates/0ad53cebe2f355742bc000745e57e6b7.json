{
  "components/common/pagination": "{{#if links.length}}\n  <nav class=\"pagination button-group\">\n    <a\n      class=\"button-group-item pagination-link previous {{#unless previous}}disabled{{/unless}}\"\n      href=\"{{previous}}\"\n    >\n      <svg class=\"icon icon-arrow-left\"><use xlink:href=\"#icon-arrow-left\" /></svg>\n    </a>\n\n    {{#each links}}\n      <a\n        class=\"button-group-item pagination-link pagination-page-link {{#if ../current \"==\" number}}active{{/if}}\"\n        href=\"{{url}}\"\n        data-pagination-current-page-link\n      >\n        {{number}}\n      </a>\n    {{/each}}\n\n    <a\n      class=\"button-group-item pagination-link next {{#unless next}}disabled{{/unless}}\"\n      href=\"{{next}}\"\n    >\n      <svg class=\"icon icon-arrow-right\"><use xlink:href=\"#icon-arrow-right\" /></svg>\n    </a>\n  </nav>\n{{/if}}\n"
}
