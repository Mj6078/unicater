{
  "components/products/product-single-tabs": "<section style=\"margin-top: -200px;\" class=\"product-tabs section\" data-product-container>\r\n  <ul class=\"tabs\" data-tabs role=\"tablist\">\r\n    {{#if theme_settings.product-additional-info}}\r\n      <li class=\"tab-title\">\r\n        <a href=\"#product-additional-info\" class=\"tab-title\" role=\"tab\" data-tab-link>\r\n          {{lang 'product.additional_info'}}\r\n        </a>\r\n      </li>\r\n    {{/if}}\r\n\r\n    <li class=\"tab-title\">\r\n      <a href=\"#product-description\" class=\"tab-title\" role=\"tab\" data-tab-link>\r\n          {{lang 'product.description'}}\r\n      </a>\r\n  </li>\r\n\r\n    {{#if product.videos.list.length}}\r\n      <li class=\"tab-title\">\r\n        <a href=\"#product-videos\" class=\"tab-title\" role=\"tab\" data-tab-link>\r\n          {{lang 'product.videos'}}\r\n        </a>\r\n      </li>\r\n    {{/if}}\r\n    <!-- {{#if settings.show_product_reviews}}\r\n      <li class=\"tab-title\">\r\n        <a href=\"#product-reviews\" class=\"tab-title\" role=\"tab\" data-tab-link>\r\n          {{lang 'product.reviews_tab_title'}}\r\n          <span\r\n            class=\"num-reviews\"\r\n            data-num-reviews=\"({{product.reviews.total}})\"\r\n          ></span>\r\n        </a>\r\n      </li>\r\n    {{/if}} -->\r\n  </ul>\r\n  <div class=\"container container-small\">\r\n\r\n    <div class=\"tabs-content\">\r\n\r\n    {{#if theme_settings.product-additional-info}}\r\n      <div class=\"accordion-title\">\r\n        <a\r\n          href=\"#product-additional-info\"\r\n          data-scroll=\"#product-additional-info\"\r\n        >\r\n          {{lang 'product.additional_info'}}\r\n          <svg class=\"icon icon-plus\">\r\n            <use xlink:href=\"#icon-plus\" />\r\n          </svg>\r\n          <svg class=\"icon icon-minus\">\r\n            <use xlink:href=\"#icon-minus\" />\r\n          </svg>\r\n        </a>\r\n      </div>\r\n      <div\r\n        class=\"tab-content-panel product-additional-info\"\r\n        id=\"product-additional-info\"\r\n        data-additional-info\r\n        data-tab-content\r\n      >\r\n        {{> components/products/additional-info}}\r\n      </div>\r\n    {{/if}}\r\n    \r\n    <div class=\"accordion-title\">\r\n      <a href=\"#product-description\" data-scroll=\"#product-description\">\r\n          {{lang 'product.description'}}\r\n          <svg class=\"icon icon-plus\">\r\n              <use xlink:href=\"#icon-plus\" />\r\n          </svg>\r\n          <svg class=\"icon icon-minus\">\r\n              <use xlink:href=\"#icon-minus\" />\r\n          </svg>\r\n      </a>\r\n  </div>\r\n  <div class=\"tab-content-panel product-description\" id=\"product-description\" data-tab-content>\r\n      {{> components/products/product-description}}\r\n  </div>\r\n\r\n\r\n      {{#if product.videos.list.length}}\r\n        <div class=\"accordion-title\">\r\n          <a href=\"#product-videos\" data-scroll=\"#product-videos\">\r\n            {{lang 'product.videos'}}\r\n            <svg class=\"icon icon-plus\">\r\n              <use xlink:href=\"#icon-plus\" />\r\n            </svg>\r\n            <svg class=\"icon icon-minus\">\r\n              <use xlink:href=\"#icon-minus\" />\r\n            </svg>\r\n          </a>\r\n        </div>\r\n        <div class=\"tab-content-panel\" id=\"product-videos\" data-tab-content>\r\n          <div class=\"product-videos-list\">\r\n            {{#each product.videos.list}}\r\n              {{> components/products/video-block video=this}}\r\n            {{/each}}\r\n          </div>\r\n        </div>\r\n      {{/if}}\r\n      {{#if settings.show_product_reviews}}\r\n        <div class=\"accordion-title\">\r\n          <a href=\"#product-reviews\" data-scroll=\"#product-reviews\">\r\n            {{lang 'product.reviews_tab_title'}}\r\n            {{#any\r\n              product.videos.list.length\r\n              theme_settings.product-additional-info\r\n            }}\r\n              <svg class=\"icon icon-plus\">\r\n                <use xlink:href=\"#icon-plus\" />\r\n              </svg>\r\n              <svg class=\"icon icon-minus\">\r\n                <use xlink:href=\"#icon-minus\" />\r\n              </svg>\r\n            {{/any}}\r\n          </a>\r\n        </div>\r\n        <div class=\"tab-content-panel\" id=\"product-reviews\" data-tab-content>\r\n\r\n          {{{snippet 'reviews'}}}\r\n\r\n          <h3 class=\"product-reviews-title\">\r\n            {{lang 'product.reviews.title' numReviews=product.num_reviews}}\r\n          </h3>\r\n          {{#if product.reviews.list.length}}\r\n            <div class=\"review-items\">\r\n              {{#each product.reviews.list}}\r\n                {{> components/products/review-item index=@index}}\r\n              {{/each}}\r\n            </div>\r\n\r\n            {{#if product.reviews.list.length '>' 2}}\r\n              <div class=\"reviews-more-wrap\">\r\n                <a class=\"reviews-show-more-link\" href=\"#\">\r\n                  {{lang 'product.reviews.show_more'}}\r\n                  {{> svg/arrow-down}}\r\n                </a>\r\n              </div>\r\n            {{/if}}\r\n          {{else}}\r\n            <h5>{{lang 'product.reviews.no_reviews'}}</h5>\r\n          {{/if}}\r\n\r\n          {{> components/products/review-form}}\r\n        </div>\r\n        <svg class=\"icon icon-spinner\">\r\n          <use xlink:href=\"#icon-spinner\" />\r\n        </svg>\r\n      </div>\r\n    {{/if}}\r\n  </div><!-- /.container-small -->\r\n</section>\r\n",
  "components/products/additional-info": "<div class=\"table-container\">\n<div class=\"new-container roll\">\n  <table class=\"new-product-details-table\">\n    <tbody>\n      {{#if product.warranty}}\n      <tr class=\"new-product-row\">\n        <td class=\"new-product-details-name\">{{lang 'product.warranty'}}:</td>\n        <td class=\"new-product-details-value\">{{product.warranty}}</td>\n      </tr>\n      {{/if}}\n\n      {{#if product.sku}}\n      <tr class=\"new-product-row\">\n        <td class=\"new-product-details-name\">{{lang 'product.sku'}}:</td>\n        <td class=\"new-product-details-value\">{{product.sku}}</td>\n      </tr>\n      {{/if}}\n\n      {{#if product.condition}}\n      <tr class=\"new-product-row\">\n        <td class=\"new-product-details-name\">{{lang 'product.condition'}}:</td>\n        <td class=\"new-product-details-value\">{{product.condition}}</td>\n      </tr>\n      {{/if}}\n\n      {{#if product.release_date}}\n      <tr class=\"new-product-row\">\n        <td class=\"new-product-details-name\">{{lang 'product.release_date'}}:</td>\n        <td class=\"new-product-details-value\">{{product.release_date}}</td>\n      </tr>\n      {{/if}}\n\n      {{#if product.availability}}\n      <tr class=\"new-product-row\">\n        <td class=\"new-product-details-name\">{{lang 'product.availability'}}:</td>\n        <td class=\"new-product-details-value\">{{product.availability}}</td>\n      </tr>\n      {{/if}}\n\n      {{#if theme_settings.product-dimensions}}\n      {{#if product.weight}}\n      <tr class=\"new-product-row\">\n        <td class=\"new-product-details-name\">{{lang 'product.weight'}}:</td>\n        <td class=\"new-product-details-value\" data-product-weight>{{product.weight}}</td>\n      </tr>\n      {{/if}}\n\n      {{#if product.height}}\n      <tr class=\"new-product-row\">\n        <td class=\"new-product-details-name\">{{lang 'product.height'}}:</td>\n        <td class=\"new-product-details-value\">{{product.height}}</td>\n      </tr>\n      {{/if}}\n\n      {{#if product.width}}\n      <tr class=\"new-product-row\">\n        <td class=\"new-product-details-name\">{{lang 'product.width'}}:</td>\n        <td class=\"new-product-details-value\">{{product.width}}</td>\n      </tr>\n      {{/if}}\n\n      {{#if product.depth}}\n      <tr class=\"new-product-row\">\n        <td class=\"new-product-details-name\">{{lang 'product.depth'}}:</td>\n        <td class=\"new-product-details-value\">{{product.depth}}</td>\n      </tr>\n      {{/if}}\n      {{/if}}\n\n      {{#if product.gift_wrapping_available}}\n      <tr class=\"new-product-row\">\n        <td class=\"new-product-details-name\">{{lang 'product.gift_wrapping'}}:</td>\n        <td class=\"new-product-details-value\">{{lang 'product.gift_wrapping_available'}}</td>\n      </tr>\n      {{/if}}\n\n      {{#if product.shipping}}\n      {{#if product.shipping.calculated}}\n      <tr class=\"new-product-row\">\n        <td class=\"new-product-details-name\">{{lang 'product.shipping'}}:</td>\n        <td class=\"new-product-details-value\">{{lang 'product.shipping_calculated'}}</td>\n      </tr>\n      {{else}}\n      {{#if (equal product.shipping.price 0)}}\n      <tr class=\"new-product-row\">\n        <td class=\"new-product-details-name\">{{lang 'product.shipping'}}:</td>\n        <td class=\"new-product-details-value\">{{lang 'product.shipping_free'}}</td>\n      </tr>\n      {{else}}\n      <tr class=\"new-product-row\">\n        <td class=\"new-product-details-name\">{{lang 'product.shipping'}}:</td>\n        <td class=\"new-product-details-value\">{{lang 'product.shipping_fixed' amount=product.shipping.price.formatted}}</td>\n      </tr>\n      {{/if}}\n      {{/if}}\n      {{/if}}\n\n      {{#each product.custom_fields}}\n      <tr class=\"new-product-row\">\n        <td class=\"new-product-details-name\">{{name}}:</td>\n        <td class=\"new-product-details-value\">{{value}}</td>\n      </tr>\n      {{/each}}\n    </tbody>\n  </table>\n</div>\n</div>\n\n\n\n<style>\n.table-container {\n  width: 750PX;\n  display: flex;\n  justify-content: center; /* Center the container horizontally */\n}\n\n.new-container {\n  width: 100%; /* Adjust width as needed */\n}\n\n.new-product-details-table {\n  width: 100%;\n  border-collapse: collapse;\n}\n\n.new-product-details-name {\n  font-weight: bold;\n  color: #000;\n  padding: 8px;\n  border-bottom: 1px solid #ddd;\n}\n\n.new-product-details-value {\n  padding: 8px;\n  border-bottom: 1px solid #ddd;\n}\n\n.new-product-row:nth-child(even) {\n  background-color: #f2f2f2; /* Even row color */\n}\n\n</style>\n",
  "components/products/video-block": "<div class=\"video-container\">\n  <iframe src=\"https://www.youtube.com/embed/{{id}}?rel=0&showinfo=0&autohide=1\" frameborder=\"0\" allowfullscreen></iframe>\n</div>\n",
  "components/products/product-description": "{{#if theme_settings.product-single-display-order '===' 'product-description-first'}}\n  <div class=\"product-description\">\n    {{{product.description}}}\n\n    {{{snippet 'product_description'}}}\n  </div>\n\n  {{> components/products/product-stock}}\n{{else}}\n  {{> components/products/product-stock}}\n\n  <div class=\"product-description\">\n    {{{product.description}}}\n\n    {{{snippet 'product_description'}}}\n  </div>\n{{/if}}\n\n{{> components/products/bulk-pricing}}\n",
  "components/products/review-item": "<article class=\"review-item {{#if index '>' 1}}hidden{{/if}}\">\n  <div class=\"review-meta\">\n    <div class=\"product-item-rating\">\n      {{> components/products/ratings rating=rating size=\"medium\"}}\n      <div class=\"review-item-score sr-only\"><span>{{rating}}</span>/5</div>\n    </div>\n    {{#if name.length}}\n      <span class=\"review-author\" content=\"{{name}}\">{{name}}</span>\n    {{/if}}\n    <span class=\"review-date\">{{date}}</span>\n  </div>\n\n  <h4 class=\"review-item-title\">{{{sanitize title}}}</h4>\n  <p class=\"review-item-body\">{{{sanitize text}}}</p>\n</article>\n",
  "svg/arrow-down": "<svg class=\"icon-arrow-down\" width=\"9\" height=\"7\" viewBox=\"0 0 9 7\" xmlns=\"http://www.w3.org/2000/svg\"><title>dropdown_arrow</title><path d=\"M1.832.753l2.668 2.7 2.67-2.7c.418-.42 1.097-.42 1.516 0 .417.424.42 1.11 0 1.533l-3.428 3.46c-.417.42-1.098.42-1.518 0L.314 2.287c-.42-.424-.42-1.11 0-1.533.42-.42 1.1-.42 1.518 0z\"/></svg>\n",
  "components/products/review-form": "<div class=\"review-form\">\n  <h3 class=\"review-form-heading\">{{lang 'product.reviews.write_review'}}</h3>\n  <form class=\"form\" id=\"form-leave-a-review\" action=\"{{urls.product.post_review}}\" method=\"post\" data-validated-form>\n    <!-- Name -->\n    <div class=\"form-field rating-name\">\n      <label class=\"form-label\" for=\"rating-name\">\n        <div class=\"form-label-text\">{{lang 'product.reviews.form.name'}}:</div>\n        <input id=\"rating-name\" class=\"form-input\" type=\"text\" placeholder=\"{{lang 'product.reviews.form.your_name'}}\" name=\"revfromname\" required>\n      </label>\n    </div>\n\n    <!-- Email -->\n    <div class=\"form-field rating-email\">\n      <label class=\"form-label\" for=\"rating-email\">\n        <div class=\"form-label-text\">{{lang 'product.reviews.form.email'}}:</div>\n        <input id=\"rating-email\" class=\"form-input\" type=\"email\" name=\"email\" placeholder=\"{{lang 'product.reviews.form.your_email'}}\" required>\n      </label>\n    </div>\n\n    <!-- Rating -->\n    <div class=\"form-field rating-number\">\n      <label class=\"form-label\" for=\"rating-stars\">\n        <div class=\"form-label-text\">{{lang 'product.reviews.form.rating_label'}}:</div>\n        <div class=\"rating-stars-wrap\">\n          <select class=\"form-input input-select rating-stars\" id=\"rating-stars\" name=\"revrating\">\n            {{#for 1 5}}\n              <option value=\"{{$index}}\" {{#if $index '===' 5}}selected{{/if}}>{{lang 'product.reviews.stars_count' NUM=$index}}</option>\n            {{/for}}\n          </select>\n          <div class=\"product-item-rating\">\n            <span class=\"rating-stars-label\">{{lang 'product.reviews.stars_count' NUM=5}}</span>\n            {{#for 1 5}}\n              <span class=\"icon-star-wrap full\">\n                <svg class=\"icon icon-star\"><use xlink:href=\"#icon-star\" /></svg>\n              </span>\n            {{/for}}\n            <svg class=\"icon icon-arrow-down\"><use xlink:href=\"#icon-arrow-down\" /></svg>\n          </div>\n        </div>\n      </label>\n    </div>\n\n    <!-- Review Subject -->\n    <div class=\"form-field rating-subject\">\n      <label class=\"form-label\" for=\"rating-title\">\n        <div class=\"form-label-text\">{{lang 'product.reviews.form.subject'}}:</div>\n        <input id=\"rating-title\" class=\"form-input\" type=\"text\" placeholder=\"{{lang 'product.reviews.form.your_subject'}}\" name=\"revtitle\" required>\n      </label>\n    </div>\n\n    <!-- Comments -->\n    <div class=\"form-field rating-comments\">\n      <label class=\"form-label\" for=\"rating-comment\">\n        <div class=\"form-label-text\">{{lang 'product.reviews.form.comments'}}:</div>\n        <textarea id=\"rating-comment\" class=\"form-input\" name=\"revtext\" placeholder=\"{{lang 'product.reviews.form.your_comments'}}\" rows=\"4\" required></textarea>\n      </label>\n    </div>\n\n    {{#if product.reviews.captcha}}\n      {{> core/forms/common/captcha captcha=product.reviews.captcha name=recaptcha_response_field}}\n    {{/if}}\n\n    <input type=\"hidden\" name=\"product_id\" value=\"{{product.id}}\" />\n    <input type=\"hidden\" name=\"action\" value=\"post_review\" />\n    {{#if product.reviews.throttleToken}}\n      <input type=\"hidden\" name=\"throttleToken\" value=\"{{product.reviews.throttleToken}}\">\n    {{/if}}\n\n    <div class=\"form-submit-wrap\">\n      <input type=\"submit\" class=\"form-submit button button-primary button-wide\" value=\"{{lang 'product.reviews.form.submit'}}\">\n    </div>\n  </form>\n</div>\n",
  "components/products/product-stock": "<div class=\"product-stock\">\n\t<span\n\t\tclass=\"{{#unless product.stock_level}}product-details-hidden{{/unless}}\"\n\t\tdata-product-stock\n\t>\n\t\t{{lang 'product.current_stock'}}\n\t</span>\n\t<span\n\t\tclass=\"{{#unless product.stock_level}}product-details-hidden{{/unless}}\"\n\t\tdata-product-stock\n\t\tdata-product-stock-level\n\t>\n\t\t{{product.stock_level}}\n\t</span>\n</div>\n",
  "components/products/bulk-pricing": "{{#if product.bulk_discount_rates.length}}\n  <div class=\"bulk-pricing-block\">\n    <h1 class=\"product-details-heading\">{{lang 'product.bulk_pricing.title'}}</h1>\n    <ul>\n      {{#each product.bulk_discount_rates}}\n        <li>\n          {{lang 'product.bulk_pricing.range' min=min max=max}}\n          {{#if type '===' 'percent'}}\n            {{lang 'product.bulk_pricing.percent' discount=discount.formatted}}\n          {{/if}}\n          {{#if type '===' 'fixed'}}\n            {{lang 'product.bulk_pricing.fixed' discount=discount.formatted}}\n          {{/if}}\n          {{#if type '===' 'price'}}\n            {{lang 'product.bulk_pricing.price' discount=discount.formatted}}\n          {{/if}}\n        </li>\n      {{/each}}\n    </ul>\n  </div>\n{{/if}}\n",
  "components/products/ratings": "{{#for 1 5}}\n  <span class=\"{{#if ../this.rating '>=' $index}}full{{/if}}\">\n    <svg class=\"icon icon-star\"><use xlink:href=\"#icon-star\" /></svg>\n  </span>\n{{/for}}\n",
  "core/forms/common/captcha": "{{!\n  Basically just a wrapper.\n  If you call this file it should serve the correct markup based on where it's called from.\n}}\n\n{{#if product.reviews.recaptcha}}\n  {{{product.reviews.recaptcha.markup}}}\n{{/if}}\n{{#if forms.create_account.recaptcha}}\n  {{{forms.create_account.recaptcha.markup}}}\n{{/if}}\n{{#if forms.contact.recaptcha}}\n  {{{forms.contact.recaptcha.markup}}}\n{{/if}}\n"
}
