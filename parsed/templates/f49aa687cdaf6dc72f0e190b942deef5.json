{
  "components/products/add-to-cart-form": "<form style=\"margin-top: -70px;\"\r\n  class=\"form add-to-cart-form\"\r\n  method=\"post\"\r\n  action=\"{{product.cart_url}}\"\r\n  enctype=\"multipart/form-data\"\r\n  data-cart-item-add\r\n  data-product-options-count=\"{{product.options.length}}\"\r\n>\r\n  <input type=\"hidden\" name=\"action\" value=\"add\">\r\n  <input type=\"hidden\" name=\"product_id\" value=\"{{product.id}}\" data-product-id/>\r\n\r\n  <div class=\"product-options-container\" data-product-option-change>\r\n    {{#if product.customizations}}\r\n      {{#each product.customizations}}\r\n        {{{dynamicComponent 'core/forms/customizations'}}}\r\n      {{/each}}\r\n    {{/if}}\r\n\r\n    {{#if product.options}}\r\n      {{#each product.options}}\r\n        {{{dynamicComponent 'core/forms/options'}}}\r\n      {{/each}}\r\n    {{/if}}\r\n  </div>\r\n\r\n  {{#if product.event_date}}\r\n    {{> core/products/event}}\r\n  {{/if}}\r\n\r\n  <div class=\"product-quantity-submit\">\r\n    <div class=\"product-quantity-container\">\r\n      {{#if theme_settings.show_product_quantity_box}}\r\n      <div class=\"form-field form-inline\">\r\n        <label class=\"form-label\">\r\n          <div class=\"product-quantity-toggle-wrapper\">\r\n            <span class=\"product-quantity-toggle product-quantity-decrement\">\r\n              <svg class=\"icon icon-minus\">\r\n                <use xlink:href=\"#icon-minus\" />\r\n              </svg>\r\n            </span>\r\n          </div>\r\n          <input\r\n            type=\"number\"\r\n            class=\"product-quantity form-input\"\r\n            name=\"qty[]\"\r\n            value=\"{{#if product.quantity}}{{product.quantity}}{{else}}{{#if product.min_purchase_quantity}}{{product.min_purchase_quantity}}{{else}}1{{/if}}{{/if}}\"\r\n            min=\"{{#if product.min_purchase_quantity}}{{product.min_purchase_quantity}}{{else}}0{{/if}}\"\r\n            {{#if product.max_purchase_quantity}}\r\n              max=\"{{product.max_purchase_quantity}}\"\r\n            {{/if}}\r\n            pattern=\"[0-9]+\"\r\n          >\r\n          <div class=\"product-quantity-toggle-wrapper\">\r\n            <span class=\"product-quantity-toggle product-quantity-increment\">\r\n              <svg class=\"icon icon-plus\">\r\n                <use xlink:href=\"#icon-plus\" />\r\n              </svg>\r\n            </span>\r\n          </div>\r\n        </label>\r\n      </div>      \r\n      {{/if}}\r\n\r\n<!--       <div class=\"share-block\">\r\n        <span class=\"form-label-text share-title\">\r\n          {{ lang 'product.share'}}\r\n        </span>\r\n        {{> components/common/share-buttons\r\n          share_buttons=settings.add_this\r\n          url=product.url\r\n          image=(getImage product.main_image 'thumb')\r\n          title=product.title\r\n        }}\r\n      </div>\r\n    </div> -->\r\n\r\n    {{#if product.can_purchase}}\r\n      <button\r\n        type=\"submit\"\r\n        class=\"\r\n          button\r\n          button-primary\r\n          button-wide\r\n          add-to-cart\r\n          button-progress\r\n          spinner\r\n        \"\r\n        data-button-purchase\r\n        data-event-type=\"product-click\"\r\n        {{#unless product.can_purchase}}\r\n          disabled\r\n        {{/unless}}\r\n      >\r\n        <span class=\"button-text\">\r\n          {{#unless product.can_purchase}}\r\n            {{lang 'product.sold_out'}}\r\n          {{else}}\r\n            {{#if product.pre_order}}\r\n              {{lang 'product.pre_order'}}\r\n            {{else if cart_icon '==' 'bag'}}\r\n              {{lang 'product.add_to_bag'}}\r\n            {{else}}\r\n              {{lang 'product.add_to_cart'}}\r\n            {{/if}}\r\n          {{/unless}}\r\n        </span>\r\n      </button>\r\n    {{/if}}\r\n\r\n    {{#if settings.show_wishlist}}\r\n    <div class=\"wishlist-form\" data-wishlist-dropdown>\r\n      {{#if customer.wishlists}}\r\n      <a\r\n      href=\"#\"\r\n      data-wishlist-toggle\r\n      >\r\n      \r\n\r\n        <img\r\n          class=\"wishlist-icon\"\r\n          style=\"height: 20px; width: 20px;\"\r\n          src=\"https://cdn11.bigcommerce.com/s-6yvytzf4lr/images/stencil/original/image-manager/wishlist-1-.png?t=1713211391\"\r\n          alt=\"Placeholder Image\"\r\n          data-wishlist-toggle\r\n        >\r\n        {{lang 'product.add_to_wishlist'}}\r\n      </a>\r\n        <ul class=\"add-to-wishlists\" data-wishlist-panel>\r\n          {{#each customer.wishlists}}\r\n            <li>\r\n              <a\r\n                href=\"{{../product.add_to_wishlist_url}}&wishlistid={{id}}\"\r\n                data-wishlist=\"{{view_url}}\"\r\n                data-is-customer\r\n              >\r\n                {{name}}\r\n              </a>\r\n            </li>\r\n          {{/each}}\r\n        </ul>\r\n      {{else}}\r\n      <a\r\n      href=\"{{product.add_to_wishlist_url}}\"\r\n    >\r\n\r\n        <img\r\n          class=\"wishlist-icon\"\r\n          style=\"height: 20px; width: 20px;\"\r\n          src=\"https://cdn11.bigcommerce.com/s-6yvytzf4lr/images/stencil/original/image-manager/wishlist-1-.png?t=1713211391\"\r\n          alt=\"Placeholder Image\"\r\n          href=\"{{product.add_to_wishlist_url}}\"\r\n        >\r\n    \r\n      </a>\r\n      {{/if}}\r\n    </div>\r\n  {{/if}}\r\n  \r\n\r\n\r\n    <!-- {{#if settings.show_wishlist}}\r\n      <div class=\"wishlist-form\" data-wishlist-dropdown>\r\n        {{#if customer.wishlists}}\r\n          <a\r\n            class=\"button button-secondary button-wide add-to-wishlist\"\r\n            href=\"#\"\r\n            data-wishlist-toggle\r\n          >\r\n            {{lang 'product.add_to_wishlist'}}\r\n          </a>\r\n          <ul class=\"add-to-wishlists\" data-wishlist-panel>\r\n            {{#each customer.wishlists}}\r\n              <li>\r\n                <a\r\n                  href=\"{{../product.add_to_wishlist_url}}&wishlistid={{id}}\"\r\n                  data-wishlist=\"{{view_url}}\"\r\n                  data-is-customer\r\n                >\r\n                  {{name}}\r\n                </a>\r\n              </li>\r\n            {{/each}}\r\n          </ul>\r\n        {{else}}\r\n          <a\r\n            class=\"button button-secondary button-wide add-to-wishlist\"\r\n            href=\"{{product.add_to_wishlist_url}}\"\r\n          >\r\n            {{lang 'product.add_to_wishlist'}}\r\n          </a>\r\n        {{/if}}\r\n      </div>\r\n    {{/if}} -->\r\n  </div>\r\n</form>",
  "core/products/event": "<div class=\"form-field form-field-date form-required\">\n  <span class=\"form-label\">\n\n    <span class=\"form-field-title\">\n      {{product.event_date.name}}\n      <span class=\"required-text\">{{lang 'common.required'}}</span>\n    </span>\n\n    <span class=\"form-field-control\">\n      <span class=\"form-select-wrapper form-date-year\">\n        <select class=\"form-input form-select\" name=\"EventDate[Yr]\" required>\n          <option disabled selected>{{lang 'forms.date.year'}}</option>\n          {{#if product.event_date.earliest_year '==' product.event_date.latest_year}}\n            <option value=\"{{product.event_date.earliest_year}}\">{{product.event_date.earliest_year}}</option>\n          {{else}}\n            {{#for product.event_date.earliest_year product.event_date.latest_year}}\n              <option value=\"{{$index}}\">{{$index}}</option>\n            {{/for}}\n          {{/if}}\n        </select>\n      </span>\n\n      <span class=\"form-select-wrapper form-date-month\">\n        <select class=\"form-input form-select\" name=\"EventDate[Mth]\" required>\n          <option disabled selected>{{lang 'forms.date.month'}}</option>\n          {{#for 1 12}}\n            <option value=\"{{$index}}\">{{getShortMonth $index}}</option>\n          {{/for}}\n        </select>\n      </span>\n\n      <span class=\"form-select-wrapper form-date-day\">\n        <select class=\"form-input form-select\" name=\"EventDate[Day]\" required>\n          <option disabled selected>{{lang 'forms.date.day'}}</option>\n          {{#for 1 31}}\n            <option value=\"{{$index}}\">{{$index}}</option>\n          {{/for}}\n        </select>\n      </span>\n    </span>\n\n  </span>\n\n  {{#if product.event_date.date_start}}\n    {{#if product.event_date.date_end}}\n      <span class=\"product-event-bounds\">{{lang 'forms.date.bounds' start_date=product.event_date.date_start end_date=product.event_date.date_end}}</span>\n    {{/if}}\n  {{/if}}\n</div>\n",
  "core/forms/customizations/file": "\n<div class=\"form-field form-field-customizations form-field-file {{#if required}}form-required{{/if}}\">\n  <label class=\"form-label\">\n    <span class=\"form-field-title\">\n      {{display_name}}\n      {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n    </span>\n    <span class=\"form-field-control\">\n      <span class=\"form-file-wrapper\" data-button-label=\"{{lang 'core.account.products.upload'}}\">\n        <input\n          class=\"form-input form-file\"\n          id=\"product-fields-{{id}}\"\n          name=\"ProductFields[{{this.id}}]\"\n          type=\"file\"\n          {{#if required}}required{{/if}}\n          aria-required=\"{{required}}\">\n      </span>\n    </span>\n  </label>\n\n  {{#if file_types '!==' ''}}\n    <span class=\"form-file-types\">{{lang 'core.account.products.filetypes' file_types=file_types}}</span>\n  {{/if}}\n\n  <span class=\"form-file-size\">{{lang 'core.account.products.max_filesize' bytes=file_size}}</span>\n</div>\n",
  "components/common/share-buttons": "{{#if share_buttons}}\n  <nav class=\"share-buttons-nav\">\n    <ul class=\"share-buttons\" data-share-buttons>\n\n      {{#each share_buttons.buttons}}\n        {{#if service '===' 'facebook'}}\n          <li class=\"social-link facebook\">\n            <a target=\"_blank\" href=\"http://www.facebook.com/sharer.php?u={{../../url}}\" class=\"facebook\">\n              {{> svg/social/facebook}}\n            </a>\n          </li>\n        {{/if}}\n\n        {{#if service '===' 'twitter'}}\n          <li class=\"social-link twitter\">\n            <a target=\"_blank\" href=\"https://twitter.com/share?url={{../../url}}&amp;text={{../../title}}\" class=\"twitter\">\n              {{> svg/social/twitter}}\n            </a>\n          </li>\n        {{/if}}\n\n        {{#if service '===' 'google_plusone'}}\n          <li class=\"social-link google-plus\">\n            <a target=\"_blank\" href=\"https://plus.google.com/share?url={{../../url}}\" class=\"google\">\n              {{> svg/social/googleplus}}\n            </a>\n          </li>\n        {{/if}}\n\n        {{#if service '===' 'linkedin'}}\n          <li class=\"social-link linkedin\">\n            <a target=\"_blank\" href=\"http://www.linkedin.com/shareArticle?url={{../../url}}&amp;title={{../../title}}\" class=\"linkedin\">\n              {{> svg/social/linkedin}}\n            </a>\n          </li>\n        {{/if}}\n\n        {{#if service '===' 'email'}}\n          <li class=\"social-link mail\">\n            <a href=\"mailto:?subject={{../../title}}&amp;body={{../../url}}\" class=\"email\">\n              {{> svg/social/email}}\n            </a>\n          </li>\n        {{/if}}\n\n        {{#if service '===' 'print'}}\n          <li class=\"social-link print\">\n            <a class=\"print\" href=\"javascript:;\" onclick=\"window.print()\">\n              {{> svg/social/print}}\n            </a>\n          </li>\n        {{/if}}\n\n        {{#if service '===' 'pinterest'}}\n          <li class=\"social-link pinterest\">\n            <a target=\"_blank\" href=\"//pinterest.com/pin/create/button/?url={{../../url}}&amp;media={{../../image}}&amp;description={{../../title}}\" class=\"share-pinterest\">\n              {{> svg/social/pinterest}}\n            </a>\n          </li>\n        {{/if}}\n      {{/each}}\n\n      {{#if settings.facebook_like_button.enabled}}\n        <iframe\n          class=\"facebook{{#if settings.facebook_like_button.verb '===' 'recommend'}}Recommend{{else}}Like{{/if}}Btn\"\n          style=\"border:none; overflow:hidden; margin: 0; padding: 0; position:absolute\"\n          src=\"https://www.facebook.com/plugins/like.php?href={{settings.facebook_like_button.href}}&amp;layout=button_count{{#if settings.facebook_like_button.verb '===' 'recommend'}}&amp;action=recommend{{/if}}&amp;colorscheme=light&amp;height=20\"\n          scrolling=\"no\"\n          frameborder=\"0\"\n          allowTransparency=\"true\">\n        </iframe>\n      {{/if}}\n    </ul>\n  </nav>\n{{/if}}\n",
  "core/forms/customizations/checkbox": "\n<div class=\"form-field form-field-customizations form-field-checkbox{{#if required}} form-required{{/if}}\">\n  <div class=\"form-field-title\">\n    {{display_name}}\n    {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n  </div>\n  <label class=\"form-label\">\n    <span class=\"form-field-control\">\n      <input class=\"form-input form-checkbox\"\n        id=\"product-field-{{id}}\"\n        name=\"ProductFields[{{this.id}}]\"\n        type=\"checkbox\"\n        {{#if value}}checked{{/if}}\n        {{#if required}}required{{/if}}\n        aria-required=\"{{required}}\"\n      >\n      <span class=\"form-label-text product-customization-text\">\n        {{display_name}}\n      </span>\n    </span>\n  </label>\n</div>\n",
  "core/forms/customizations/select": "\n<div class=\"form-field form-field-customizations form-field-select{{#if required}} form-required{{/if}}\">\n  <label class=\"form-label\">\n    <span class=\"form-field-title\">\n      {{display_name}}\n      {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n    </span>\n    <span class=\"form-field-control\">\n      <span class=\"form-select-wrapper\">\n        <select\n          class=\"form-input input-select\"\n          id=\"product-fields-{{id}}\"\n          name=\"ProductFields[{{this.id}}]\"\n          {{#if required}}required{{/if}}\n          aria-required=\"{{required}}\">\n          <option value=\"\" {{#any values selected=true}}{{else}}selected{{/any}} disabled>{{lang 'product.pick_one'}}</option>\n          {{#each values}}\n            <option value=\"{{this}}\" {{#if selected}}selected{{/if}}>{{this}}</option>\n          {{/each}}\n        </select>\n      </span>\n    </span>\n  </label>\n</div>\n",
  "core/forms/options/input-checkbox": "<div\n  class=\"\n    form-field\n    form-field-options\n    form-field-checkbox\n    {{#if required}}\n      form-required\n    {{/if}}\n  \"\n  data-product-attribute=\"input-checkbox\"\n>\n  <div class=\"form-field-title\">\n    {{display_name}}\n    {{#if required}}\n      <span class=\"required-text\">{{lang 'common.required'}}</span>\n    {{/if}}\n  </div>\n\n  <div class=\"form-field-control\">\n    <label class=\"form-label {{class}}\">\n      <input\n        class=\"form-input form-checkbox\"\n        id=\"attribute-{{id}}\"\n        name=\"attribute[{{id}}]\"\n        type=\"checkbox\"\n        value=\"{{value}}\"\n        {{#if checked}}\n          checked\n          data-default\n        {{/if}}\n        {{#if required}}required{{/if}}\n        aria-required=\"{{required}}\"\n      >\n      <span class=\"form-label-text\">{{label}}</span>\n    </label>\n  </div>\n</div>\n",
  "core/forms/customizations/text": "\n<div class=\"form-field form-field-customizations form-field-text{{#if required}} form-required{{/if}}\">\n  <label class=\"form-label\" for=\"product-fields-{{id}}\">\n    <span class=\"form-field-title\">\n      {{display_name}}\n      {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n    </span>\n    <span class=\"form-field-control\">\n      <input class=\"form-input\"\n        id=\"product-fields-{{id}}\"\n        name=\"ProductFields[{{this.id}}]\"\n        type=\"text\"\n        placeholder=\"{{prefill}}\"\n        value=\"{{value}}\"\n        {{#if required}}required{{/if}}\n        aria-required=\"{{required}}\">\n    </span>\n  </label>\n</div>\n",
  "core/forms/options/input-file": "<div\n  class=\"\n    form-field\n    form-field-options\n    form-field-file\n    {{#if required}}\n      form-required\n    {{/if}}\n  \"\n  data-product-attribute=\"input-file\"\n>\n  <label class=\"form-label\">\n    <span class=\"form-field-title\">\n      {{display_name}}\n      {{#if required}}\n        <span class=\"required-text\">{{lang 'common.required'}}</span>\n      {{/if}}\n    </span>\n    <span class=\"form-field-control\">\n      <span\n        class=\"form-file-wrapper\"\n        data-button-label=\"{{lang 'core.account.products.upload'}}\"\n      >\n        <input\n          class=\"form-input form-file\"\n          id=\"attribute-{{id}}\"\n          name=\"attribute[{{id}}]\"\n          type=\"file\"\n          {{#if required}}required{{/if}}\n          aria-required=\"{{required}}\"\n        >\n      </span>\n    </span>\n  </label>\n\n  {{#if file_types '!==' ''}}\n    <span class=\"form-file-types\">\n      {{lang 'core.account.products.filetypes' file_types=file_types}}\n    </span>\n  {{/if}}\n\n  <span class=\"form-file-size\">\n    {{lang 'core.account.products.max_filesize' bytes=file_size}}\n  </span>\n</div>\n",
  "core/forms/options/date": "<div\n  class=\"\n    form-field\n    form-field-options\n    form-field-date\n    {{#if required}}\n      form-required\n    {{/if}}\n  \"\n  data-product-attribute=\"date\"\n>\n  <span class=\"form-label\">\n    <span class=\"form-field-title\">\n      {{display_name}}\n      {{#if required}}\n        <span class=\"required-text\">{{lang 'common.required'}}</span>\n      {{/if}}\n    </span>\n\n    <span class=\"form-field-control\">\n      <span class=\"form-select-wrapper form-date-year\">\n        <select\n          class=\"form-input form-select\"\n          name=\"attribute[{{id}}][year]\"\n          {{#if required}}\n            required\n          {{/if}}\n          aria-required=\"{{required}}\"\n        >\n          <option selected disabled>\n            {{lang 'forms.date.year'}}\n          </option>\n          {{#if earliest_year '===' latest_year}}\n            <option value=\"{{earliest_year}}\" selected=\"selected\">\n              {{earliest_year}}\n            </option>\n          {{else}}\n            {{#for earliest_year latest_year}}\n              <option\n                value=\"{{$index}}\"\n                {{#if ../selected_date.year '==' $index}}\n                  selected=\"selected\"\n                {{/if}}\n              >\n                {{$index}}\n              </option>\n            {{/for}}\n          {{/if}}\n        </select>\n      </span>\n\n      <span class=\"form-select-wrapper form-date-month\">\n        <select\n          class=\"form-input form-select\"\n          name=\"attribute[{{id}}][month]\"\n          {{#if required}}\n            required\n          {{/if}}\n          aria-required=\"{{required}}\"\n        >\n          <option selected disabled>{{lang 'forms.date.month'}}</option>\n          {{#for 1 12}}\n            <option\n              value=\"{{$index}}\"\n              {{#if ../selected_date.month '==' $index}}\n                selected=\"selected\"\n              {{/if}}\n            >\n              {{getShortMonth $index}}\n            </option>\n          {{/for}}\n        </select>\n      </span>\n\n      <span class=\"form-select-wrapper form-date-day\">\n        <select\n          class=\"form-input form-select\"\n          name=\"attribute[{{id}}][day]\"\n          {{#if required}}\n            required\n          {{/if}}\n          aria-required=\"{{required}}\"\n        >\n          <option selected disabled>{{lang 'forms.date.day'}}</option>\n          {{#for 1 31}}\n            <option\n              value=\"{{$index}}\"\n              {{#if ../selected_date.day '==' $index}}\n                selected=\"selected\"\n              {{/if}}\n            >\n              {{$index}}\n            </option>\n          {{/for}}\n        </select>\n        </span>\n    </span>\n\n  </span>\n</div>\n",
  "core/forms/customizations/textarea": "\n<div class=\"form-field form-field-customizations form-field-textarea{{#if required}} form-required{{/if}}\">\n  <label class=\"form-label\">\n    <span class=\"form-field-title\">\n      {{display_name}}:\n      {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n    </span>\n    <span class=\"form-field-control\">\n      <textarea\n        class=\"form-input\"\n        id=\"product-fields-{{id}}\"\n        name=\"ProductFields[{{this.id}}]\"\n        placeholder=\"{{prefill}}\"\n        rows=\"4\"\n        {{#if required}}required{{/if}}\n        aria-required=\"{{required}}\"></textarea>\n    </span>\n  </label>\n</div>\n",
  "core/forms/options/input-numbers": "<div\n  class=\"\n    form-field\n    form-field-options\n    form-field-number\n    {{#if required}}\n      form-required\n    {{/if}}\n  \"\n  data-product-attribute=\"input-number\"\n>\n  <label class=\"form-label\">\n    <span class=\"form-field-title\">\n      {{display_name}}\n      {{#if required}}\n        <span class=\"required-text\">{{lang 'common.required'}}</span>\n      {{/if}}\n    </span>\n    <span class=\"form-field-control\">\n      <input\n        class=\"form-input\"\n        id=\"attribute-{{id}}\"\n        name=\"attribute[{{id}}]\"\n        type=\"number\"\n        value=\"{{prefill}}\"\n        {{#if required}}required{{/if}}\n        aria-required=\"{{required}}\"\n      >\n    </span>\n  </label>\n</div>\n",
  "core/forms/options/input-text": "<div\n  class=\"\n    form-field\n    form-field-options\n    form-field-text\n    {{#if required}}\n      form-required\n    {{/if}}\n  \"\n  data-product-attribute=\"input-text\"\n>\n  <label class=\"form-label\">\n    <span class=\"form-field-title\">\n      {{display_name}}\n      {{#if required}}\n        <span class=\"required-text\">{{lang 'common.required'}}</span>\n      {{/if}}\n    </span>\n    <span class=\"form-field-control\">\n      <input class=\"form-input\"\n        id=\"attribute-{{id}}\"\n        name=\"attribute[{{id}}]\"\n        type=\"text\"\n        value=\"{{prefill}}\"\n        {{#if required}}required{{/if}}\n        aria-required=\"{{required}}\"\n      >\n    </span>\n  </label>\n</div>\n",
  "core/forms/options/product-list": "<div\n  class=\"\n    form-field\n    form-field-options\n    form-field-product-list\n    {{#if required}}\n      form-required\n    {{/if}}\n  \"\n  data-product-attribute=\"product-list\"\n>\n  <div class=\"form-field-title\">\n    {{display_name}}\n    {{#if required}}\n      <span class=\"required-text\">{{lang 'common.required'}}</span>\n    {{/if}}\n  </div>\n\n  <div class=\"form-field-control\">\n    {{#unless values.0.image}}\n      {{!-- same as radio buttons if no image set --}}\n      {{#each values}}\n        <label class=\"form-label\" data-product-attribute-value=\"{{id}}\">\n          <input\n            class=\"form-input form-radio\"\n            name=\"attribute[{{../id}}]\"\n            type=\"radio\"\n            value=\"{{id}}\"\n            {{#if selected}}\n              checked\n              data-default\n            {{/if}}\n            {{#if ../required}}required{{/if}}\n            aria-required=\"{{required}}\"\n          >\n          <span class=\"form-label-text\">{{label}}</span>\n        </label>\n      {{/each}}\n      {{#unless required}}\n        <label class=\"form-label\">\n          <input\n            class=\"form-input form-radio\"\n            name=\"attribute[{{id}}]\"\n            type=\"radio\"\n            value=\"0\"\n            {{#if prefill '==' 0}}checked{{/if}}\n            {{#if ../required}}required{{/if}}\n            aria-required=\"{{required}}\"\n          >\n          <span class=\"form-label-text\">{{lang 'core.product.none'}}</span>\n        </label>\n      {{/unless}}\n    {{else}}\n      {{#each values}}\n        <label\n          class=\"form-label product-picklist-item\"\n          data-product-attribute-value=\"{{id}}\"\n        >\n          <input\n            class=\"form-input form-radio product-picklist-radio\"\n            name=\"attribute[{{../id}}]\"\n            type=\"radio\"\n            value=\"{{id}}\"\n            {{#if selected}}\n              checked\n              data-default\n            {{/if}}\n            {{#if ../required}}required{{/if}}\n            aria-required=\"{{required}}\"\n          >\n          <span class=\"product-picklist-item-body\">\n            <span class=\"product-picklist-image-wrap\">\n              <img\n                class=\"product-picklist-image\"\n                src=\"{{getImage image 'core-product-picklist' (cdn ../theme_settings.default_image)}}\"\n                alt=\"{{image.alt}}\"\n              >\n            </span>\n            <span class=\"product-picklist-label-text form-label-text\">\n              {{label}}\n            </span>\n          </span>\n        </label>\n      {{/each}}\n      {{#unless required}}\n        <label class=\"form-label product-picklist-item\">\n          <input\n            class=\"form-input form-radio product-picklist-radio\"\n            name=\"attribute[{{id}}]\"\n            type=\"radio\"\n            value=\"0\"\n            {{#if prefill '==' 0}}checked{{/if}}\n          >\n          <span class=\"product-picklist-item-body picklist-none\">\n            <span class=\"product-picklist-label-text form-label-text\">\n              {{lang 'product.none'}}\n            </span>\n          </span>\n        </label>\n      {{/unless}}\n    {{/unless}}\n  </div>\n</div>\n",
  "core/forms/options/set-rectangle": "<div\n  class=\"\n    form-field\n    form-field-options\n    form-field-rectangle\n    {{#if required}}\n      form-required\n    {{/if}}\n  \"\n  data-product-attribute=\"set-rectangle\"\n>\n  <div class=\"form-field-title\">\n    {{display_name}}\n    {{#if required}}\n      <span class=\"required-text\">{{lang 'common.required'}}</span>\n    {{/if}}\n  </div>\n  <div class=\"form-field-control\">\n    {{#each values}}\n      <label class=\"form-label rectangle\" data-product-attribute-value=\"{{id}}\">\n        <input\n          class=\"form-input form-rectangle\"\n          name=\"attribute[{{../id}}]\"\n          id=\"attribute-{{id}}\"\n          type=\"radio\"\n          value=\"{{id}}\"\n          {{#if selected}}\n            checked\n            data-default\n          {{/if}}\n          {{#if ../required}}required{{/if}}\n          aria-required=\"{{../required}}\"\n        >\n        <span class=\"rectangle-text form-label-text\">{{label}}</span>\n      </label>\n    {{/each}}\n    {{#unless required}}\n      <label class=\"form-label rectangle\">\n        <input\n          class=\"form-input form-rectangle\"\n          name=\"attribute[{{../id}}]\"\n          type=\"radio\"\n          value=\"0\"\n          {{#if selected}}checked{{/if}}\n          aria-required=\"{{required}}\"\n        >\n        <span class=\"rectangle-text form-label-text\">\n          {{lang 'core.product.none'}}\n        </span>\n      </label>\n    {{/unless}}\n  </div>\n</div>\n",
  "core/forms/options/set-radio": "<div\n  class=\"\n    form-field\n    form-field-options\n    form-field-radio\n    {{#if required}}\n      form-required\n    {{/if}}\n  \"\n  data-product-attribute=\"set-radio\"\n>\n  <div class=\"form-field-title product-option-title\">\n    {{display_name}}\n    {{#if required}}\n      <span class=\"required-text\">{{lang 'common.required'}}</span>\n    {{/if}}\n  </div>\n  <div class=\"form-field-control\">\n    {{#each values}}\n      <label class=\"form-label\" data-product-attribute-value=\"{{id}}\">\n        <input\n          class=\"form-input form-radio\"\n          id=\"attribute-{{id}}\"\n          name=\"attribute[{{../id}}]\"\n          type=\"radio\"\n          value=\"{{id}}\"\n          {{#if selected}}\n            checked\n            data-default\n          {{/if}}\n          {{#if ../required}}required{{/if}}\n          aria-required=\"{{../required}}\"\n        >\n        <span class=\"form-label-text\">{{label}}</span>\n      </label>\n    {{/each}}\n    {{#unless required}}\n      <label class=\"form-label\">\n        <input\n          class=\"form-input form-radio\"\n          name=\"attribute[{{../id}}]\"\n          type=\"radio\"\n          value=\"0\"\n          {{#if prefill '==' 0}}checked{{/if}}\n          aria-required=\"{{required}}\"\n        >\n        <span class=\"form-label-text\">{{lang 'core.product.none'}}</span>\n      </label>\n    {{/unless}}\n  </div>\n</div>\n",
  "core/forms/options/set-select": "<div\n  class=\"\n    form-field\n    form-field-options\n    form-field-select\n    {{#if required}}\n      form-required\n    {{/if}}\n  \"\n  data-product-attribute=\"set-select\"\n>\n  <label class=\"form-label\">\n    <span class=\"form-field-title\">\n      {{display_name}}\n      {{#if required}}\n        <span class=\"required-text\">{{lang 'common.required'}}</span>\n      {{/if}}\n    </span>\n    <span class=\"form-field-control\">\n      <div class=\"form-select-wrapper\">\n        <select\n          class=\"form-input form-select\"\n          id=\"attribute-{{id}}\"\n          name=\"attribute[{{id}}]\"\n          {{#if ../required}}required{{/if}}\n          aria-required=\"{{../required}}\"\n        >\n          <option\n            value=\"\"\n            {{#any values selected=true}}\n            {{else}}\n              selected\n            {{/any}}\n          >\n            {{lang 'product.pick_one'}}\n          </option>\n          {{#each values}}\n            <option\n              value=\"{{id}}\"\n              {{#if selected}}selected data-default{{/if}}\n              data-product-attribute-value=\"{{id}}\"\n            >\n              {{label}}\n            </option>\n          {{/each}}\n          {{#unless required}}\n            <option\n              value=\"{{id}}\"\n              {{#if selected}}selected{{/if}}\n              data-product-attribute-value=\"{{id}}\"\n            >\n              {{lang 'core.product.none'}}\n            </option>\n          {{/unless}}\n        </select>\n      </div>\n    </span>\n  </label>\n</div>\n",
  "core/forms/options/swatch": "<div\n  class=\"\n    form-field\n    form-field-options\n    form-field-swatch\n    {{#if required}}\n      form-required\n    {{/if}}\n  \"\n  data-swatch-selector\n  data-product-attribute=\"swatch\"\n>\n  <div class=\"form-field-title\">\n    {{display_name}}\n    <span class=\"swatch-value\" data-swatch-value>\n      {{lang 'forms.swatches_none_selected'}}\n    </span>\n    {{#if required}}\n      <span class=\"required-text\">{{lang 'common.required'}}</span>\n    {{/if}}\n  </div>\n\n  <div class=\"form-field-control\">\n    {{#each values}}\n      <label\n        class=\"swatch-wrap\"\n        data-swatch-value=\"{{label}}\"\n        data-product-attribute-value=\"{{id}}\"\n      >\n        <span class=\"form-label-text\">{{label}}</span>\n\n        <input\n          class=\"form-input swatch-radio\"\n          id=\"attribute-{{id}}\"\n          type=\"radio\"\n          name=\"attribute[{{../id}}]\"\n          value=\"{{id}}\"\n          {{#if selected}}\n            checked\n            data-default\n          {{/if}}\n          {{#if ../required}}required{{/if}}\n          aria-required=\"{{../required}}\"\n        >\n\n        <span\n          class=\"\n            swatch\n            {{#if data.[1]}}two-colors{{/if}}\n            {{#if data.[2]}}three-colors{{/if}}\"\n          >\n          {{#if pattern}}\n            <span\n              class=\"swatch-color swatch-pattern\"\n              style=\"background-image: url('{{getImage image 'core-swatch'}}');\"\n            >\n            </span>\n          {{/if}}\n\n          {{#if data.[0]}}\n            <span\n              class=\"swatch-color\"\n              style=\"background-color: #{{data.[0]}}\"\n            >\n            </span>\n          {{/if}}\n\n          {{#if data.[1]}}\n            <span\n              class=\"swatch-color secondary\"\n              style=\"background-color: #{{data.[1]}}\"\n            >\n            </span>\n          {{/if}}\n\n          {{#if data.[2]}}\n            <span\n              class=\"swatch-color tertiary\"\n              style=\"background-color: #{{data.[2]}}\"\n            >\n            </span>\n          {{/if}}\n        </span>\n\n        {{#if pattern}}\n          <span class=\"swatch-pattern-expanded\">\n            <img\n              class=\"swatch-pattern-image\"\n              src=\"{{getImage image 'core-swatch'}}\"\n            >\n          </span>\n        {{/if}}\n\n      </label>\n    {{/each}}\n    {{#unless required}}\n      <label\n        class=\"swatch-wrap swatch-none\"\n        data-swatch-value=\"{{lang 'core.product.none'}}\"\n        data-product-attribute-value=\"{{id}}\"\n      >\n        <span class=\"form-label-text\">{{lang 'core.product.none'}}</span>\n        <input\n          class=\"form-input swatch-radio\"\n          name=\"attribute[{{../id}}]\"\n          type=\"radio\"\n          value=\"0\"\n          {{#if prefill '==' 0}}checked{{/if}}\n          aria-required=\"{{required}}\"\n        >\n        <span class=\"swatch\">\n          <svg\n            width=\"50\"\n            height=\"50\"\n            viewBox=\"0 0 50 50\"\n            xmlns=\"http://www.w3.org/2000/svg\"\n          >\n            <path d=\"M46.717.5633l2.7197 2.7196L3.283 49.4366.5632 46.717\" fill-rule=\"nonzero\" fill=\"#D0021B\"/>\n          </svg>\n        </span>\n      </label>\n    {{/unless}}\n  </div>\n</div>\n",
  "core/forms/options/textarea": "<div\n  class=\"\n    form-field\n    form-field-options\n    form-field-textarea\n    {{#if required}}\n      form-required\n    {{/if}}\n  \"\n  data-product-attribute=\"textarea\"\n>\n  <label class=\"form-label\">\n    <span class=\"form-field-title\">\n      {{display_name}}\n      {{#if required}}\n        <span class=\"required-text\">{{lang 'common.required'}}</span>\n      {{/if}}\n    </span>\n    <span class=\"form-field-control\">\n      <textarea\n        class=\"form-input\"\n        id=\"attribute-{{id}}\"\n        name=\"attribute[{{id}}]\"\n        rows=\"4\"\n        placeholder=\"{{prefill}}\"\n        {{#if required}}required{{/if}}\n        aria-required=\"{{required}}\"\n      ></textarea>\n    </span>\n  </label>\n</div>\n",
